    "image_with_text_AdJYim": {
      "type": "image-with-text",
      "blocks": {
        "heading_Wj6eew": {
          "type": "heading",
          "settings": {
            "heading": "Fit for you.",
            "heading_fs": "h3"
          }
        },
        "sub_heading_w8QBky": {
          "type": "sub_heading",
          "settings": {
            "sub_heading": "You could play and skate perfectly fine on the manufacturer’s profile. However, the problem is that those profiles are stock; they aren’t quite personalized to fit you.<br><br>\n\nProfiling considers several aspects of you and your play, including your:<br><br>\n\n- Height and Weight<br>\n- Hollow (sharpening style)<br>\n- Player position and skill level<br>\n- Environment (hardness of the ice)<br>\n- Previous profile (what you’re used to)<br>\n- Skate brand\/family\/holder size\n- Foot position (how you walk, skate, etc.), leg shape<br>\n- Arm length (shoulder to floor distance)<br>\n- Goals for improving your skating<br>\n- Current balance and base position, maneuverability, speed, style (*goalies)",
            "sub_fs": "lg"
          }
        }
      },
      "block_order": [
        "heading_Wj6eew",
        "sub_heading_w8QBky"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/DSCF2122-2.jpg",
        "style_image": "1",
        "image_effect": "zoom-in",
        "color_scheme": "",
        "image_last": false,
        "content_position": "center",
        "alignment": "center",
